package cosmetic.modelo.entidad;

import java.util.List;

public abstract class ElementoCosmetico {

	private String nombre;
	private List<AtributoCosmetico> propiedadesCosmeticas;
	private String comentario;

	@Override
	public String toString() {
		return "ElementoCosmetico [nombre=" + nombre + ", propiedadesCosmeticas=" + propiedadesCosmeticas
				+ ", comentario=" + comentario + "]";
	}
	
	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public List<AtributoCosmetico> getPropiedadesCosmeticas() {
		return propiedadesCosmeticas;
	}

	public void setPropiedadesCosmeticas(List<AtributoCosmetico> propiedadesCosmeticas) {
		this.propiedadesCosmeticas = propiedadesCosmeticas;
	}

	public String getComentario() {
		return comentario;
	}

	public void setComentario(String comentario) {
		this.comentario = comentario;
	}

	

	

}
